<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.canvas.Canvas?>
<Pane prefHeight="400.0"
      prefWidth="500.0"
      stylesheets="@style.css"
      fx:controller="client.Controller"
      xmlns:fx="http://javafx.com/fxml">
    <children>
        <Button layoutX="10.0" layoutY="10.0" onAction="#btnConnectClick" text="Приссоединиться к серверу" />

        <Canvas layoutX="10.0" layoutY="50.0" fx:id="canvasMyField" width="200.0" height="200.0"/>
        <Canvas layoutX="220.0" layoutY="50.0" fx:id="canvasShootField" width="200.0" height="200.0" onMouseClicked="#canvasShootFieldClicked"/>
    </children>
</Pane>